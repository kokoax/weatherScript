            function displayInfobox(e) {
                if (e.targetType == 'pushpin') {
                    infobox.setLocation(e.target.getLocation());
                    infobox.setHtmlContent(e.target.Description);
                    infobox.setOptions({ visible: true, zIndex: 999 });
                }
            }

            function hideInfobox(e) {
                infobox.setOptions({ visible: false });
            }

            function newLocationPin(location, iconCode, temperature) {
                zindex = Math.round(900 - Math.abs(location.latitude * 10));
                // Shared, "default" pushpin options
                // Two will be created - one is the pretty custom HTML which causes infoboxes to not behave properly
                // The other will be a transparent icon that is z-indexed over top to get the desired infobox behavior
                // Bing Maps FTW!
                var pushpinOptions = {
                    width: 48,
                    height: 64,
                    draggable: false,
                    zIndex: zindex,
                    icon: null,
                    htmlContent: null,
                    anchor: new Microsoft.Maps.Point(24, 64)
                };
                // custom HTML
                pushpinOptions.htmlContent = "<div style=\"width:48px;height:64px;background:url(https://vortex.accuweather.com/adc2010/images/slate/icons/maps/pushpin.png);\"><div class=\"map-tooltip\"><div class=\"icon i-" + iconCode + "-s\"></div><span>" + temperature + "&deg;</span><div class=\"point\"></div></div></div>";
                mapDataLayer.push(new Microsoft.Maps.Pushpin(location, pushpinOptions));
                // transparent clickable icon
                pushpinOptions.htmlContent = null;
                pushpinOptions.icon = "https://vortex.accuweather.com/adc2010/images/slate/icons/maps/pushpin-transparent.png";
                return new Microsoft.Maps.Pushpin(location, pushpinOptions);
            }

        </script>



