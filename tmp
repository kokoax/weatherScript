        <script type="text/javascript">
            var map, infobox, mapDataLayer;
            var clickablePinArray = new Array();
            var pinLocations = new Array();

            function GetMap() {
                map = new Microsoft.Maps.Map(document.getElementById("map_canvas"), { credentials: "AoznCIbkg0lP0cZOHAPwZJMxL_Qqm9xrHj4T1BzmC2ACRGZLWHyOJIDTSakVRzT7", mapTypeId: Microsoft.Maps.MapTypeId.aerial, showMapTypeSelector: false, showScalebar: false });
                map.setView({ center: new Microsoft.Maps.Location(39.7, 141.15), zoom: 6 });

                mapDataLayer = new Microsoft.Maps.EntityCollection();
                mapDataLayer.setOptions({ zIndex: 98 });
                map.entities.push(mapDataLayer);

                var infoboxLayer = new Microsoft.Maps.EntityCollection();
                infoboxLayer.setOptions({ zIndex: 99 });
                map.entities.push(infoboxLayer);

                infobox = new Microsoft.Maps.Infobox(new Microsoft.Maps.Location(0, 0), { width: 260, height: 274, showCloseButton: false, showPointer: false, visible: false });
                //infobox = new Microsoft.Maps.Infobox(new Microsoft.Maps.Location(0, 0), { width: 258, height: 219, showCloseButton: false, offset: new Microsoft.Maps.Point(-21, 219), showPointer: true, visible: false });
                infoboxLayer.push(infobox);

                AddData();

                if (clickablePinArray.length > 1) {
                    map.setView({ bounds: Microsoft.Maps.LocationRect.fromLocations(pinLocations) });
                }

                // work-around for Bing Maps infobox y/latitude bug: set infobox offset again on map change
                Microsoft.Maps.Events.addHandler(map, 'viewchangeend', function() { infobox.setOptions({ offset: {x: -21, y: 219 } }); });

                Microsoft.Maps.Events.addHandler(map, 'mousewheel', function(e) {
                    if(e.targetType == 'map')
                    {
                        e.handled = true;
                    }
                });

            }

